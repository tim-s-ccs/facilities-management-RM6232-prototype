    "browser-sync": "^2.11.1",
    "ccs-prototype-kit-model-interface": "^0.5.0",
    "client-sessions": "^0.8.0",
    "cross-spawn": "^7.0.2",
    "del": "^6.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.13.0",
    "fancy-log": "^1.3.3",
    "govuk_frontend_toolkit": "^7.5.0",
    "govuk_template_jinja": "^0.24.1",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^4.0.0",
    "gulp": "^4.0.0",
    "gulp-nodemon": "^2.5.0",
