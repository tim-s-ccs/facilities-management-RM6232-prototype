    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jquery": "^3.5.8",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.4",
    "jest": "^27.3.1",
    "standard": "^14.3.3",
    "supertest": "^6.1.6",
